{
  "entryFile": "src/index.ts",
  "noImplicitAdditionalProperties": "silently-remove-extras",
  "controllerPathGlobs": ["src/controllers/*.ts"],
  "spec": {
    "outputDirectory": "build",
    "specVersion": 3,
    "securityDefinitions": {
      "idToken": {
          "type": "jwt"
      }
    }
  },
  "routes": {
    "routesDir": "build",
    "authenticationModule": "src/middlewares/auth.ts"
  }
}
